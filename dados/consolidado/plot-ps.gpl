#!/usr/bin/gnuplot
reset
set terminal pdf

ntics = 100
stats log_name using 1 name 'x' nooutput
intervalx = x_max/ntics
#set xtics intervalx

set xdata time
set timefmt "%s"
set format x "%H:%M:%S"
set xtics rotate by -45

set xlabel "tempo (HH:MM:SS)"
set ylabel "recursos"
set title tname
set key reverse Left outside
set grid
#set style data linespoints
set style data lines

plot log_name using 1:6 title "mem√≥ria", \
"" using 1:7 title "%cpu", \
"" using 1:10 title "#threads"
